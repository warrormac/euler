#include <iostream>

using namespace std;

int main()
{
	int x,numero;
	cout<<"ingresa 1 numero"<<endl;
	cin>>x;
	for(int y=1;;y++)
    {
        int c=0;
            for(int z=1;z<=x;z++){
                if(y%z==0){
                    c++;
                }
            }
        if(c==x)
        {
            numero=y;
            break;
        }
	}
	cout<<"menor numero divisible es "<<numero;

}
